<h1>Login</h1>

<% form_for @user_session do |f| %>
  <%= f.error_messages(:header_message => "Could not log in") %>

  <p>
    <%= f.label :username %><br />
    <%= f.text_field :username %>
  </p>
  <p>
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </p>
  <p>
    <%= f.submit "Submit" %>
  </p>
<% end %>
