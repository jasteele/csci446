<h2>Users</h2>
<% will_paginate @users %>
<table>
  <tr>
	<th>Name</th>
    <th>Username</th>
    <th>Last Login</th>
    <th>Number of Games</th>
  </tr>

<% @users.each do |user| %>
  <tr>
	<td><%= link_to user.first_last, edit_admin_user_path(user) %></td>
    <td><%=h user.username %></td>
    <td><%=h time_ago_in_words(user.last_login_at) %></td> 
    <td><%=h user.games.count%></td>
  </tr>
<% end %>
</table>
